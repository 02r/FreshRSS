<?php

$output = Request::param ('output', 'normal');

if ($output == 'rss') {
	$this->renderHelper ('rss_view');
} elseif($output == 'reader') {
	$this->renderHelper ('reader_view');
} elseif($output == 'global') {
	$this->renderHelper ('global_view');
} else {
	$this->renderHelper ('normal_view');
}